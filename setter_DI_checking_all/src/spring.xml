<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="c" class="bean.Car" dependency-check="all">
	<!-- no need to call primitive setter methods and secondary setter methods  -->
	<property name="carname" value="Benz"/>
	<property name="engine" ref="e"/>
	</bean>
	<bean id="e" class="bean.Engine" dependency-check="all">
	<property name="modelYear" value="2017"/>
	</bean>
	
</beans>
		  