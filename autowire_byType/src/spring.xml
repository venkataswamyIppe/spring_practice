<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
<!-- in byType autowiring their is chance get ambiguty to over come by autowire-candidate -->
	<bean id="e1" class="bean.Engine">
		<property name="modelYear" value="2017" />
	</bean>
	<bean id="e2" class="bean.Engine" autowire-candidate="false">
		<property name="modelYear" value="2016"/>
	</bean>
	<bean id="e3" class="bean.Engine" autowire-candidate="false">
		<property name="modelYear" value="2015" />
	</bean>
	<bean id="c" class="bean.Car" autowire="byType">
	</bean>
</beans>
		  