<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="e1" class="bean.Engine">
		<property name="modelYear" value="2017" />
	</bean>
	<bean id="e2" class="bean.Engine">
		<property name="modelYear" value="2016" />
	</bean>
	<bean id="e3" class="bean.Engine">
		<property name="modelYear" value="2015" />
	</bean>
	<bean id="c" class="bean.Car">
	<property name="carname" value="Audi"/>
		<property name="engine">
			<list>
			<ref bean="e1"/>
			<ref bean="e3"/>
			<ref bean="e2"/>
			</list>
		</property>
	</bean>
</beans>
		  